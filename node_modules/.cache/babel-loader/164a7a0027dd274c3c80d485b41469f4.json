,KAAA,CAAA,SAAA,CAAA,CAAA;AACA,OAFA,MAEA,IAAA,KAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,QAAA,QAAA,CAAA,IAAA,KAAA,CAAA,WAAA,CAAA,CAAA;AACA,OAFA,MAEA,IAAA,KAAA,KAAA,KAAA,CAAA,cAAA,CAAA,OAAA,EAAA;AACA,QAAA,QAAA,CAAA,IAAA,KAAA,CAAA,YAAA,CAAA,CAAA;AACA,OAFA,MAEA;AACA,QAAA,QAAA;AACA;AACA,KAVA;;AAYA,WAAA;AACA,MAAA,QAAA,EAAA,KAAA,MAAA,CAAA,QADA;AAGA,MAAA,SAAA,EAAA,EAHA;AAGA;AACA,MAAA,iBAAA,EAAA,KAJA;AAIA;AAEA,MAAA,cAAA,EAAA,MANA;;AAQA;AACA,MAAA,cAAA,EAAA;AACA,QAAA,OAAA,EAAA,EADA;AAEA,QAAA,OAAA,EAAA,EAFA;AAGA,QAAA,SAAA,EAAA;AAHA,OATA;;AAcA;AACA,MAAA,WAAA,EAAA;AACA,QAAA,OAAA,EAAA,CACA;AAAA,UAAA,SAAA,EAAA,eAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,CADA;AAIA,QAAA,OAAA,EAAA,CACA;AAAA,UAAA,SAAA,EAAA,eAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,CAJA;AAOA,QAAA,SAAA,EAAA,CAAA;AACA;AAAA,UAAA,SAAA,EAAA,iBAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA;AAPA;AAfA,KAAA;AA2BA,GAlEA;AAmEA,EAAA,OAAA,oBACA,UAAA,CAAA;AACA,IAAA,SAAA,EAAA,uBADA;AACA;AACA,IAAA,WAAA,EAAA;AAFA,GAAA,CADA;AAMA;AACA,IAAA,QAPA,sBAOA;AAAA;;AACA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,WAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,QAAA,MAAA,CAAA,SAAA;;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AAEA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,SADA;AAEA,YAAA,QAAA,EAAA,IAFA;AAGA,YAAA,OAAA,EAAA,OAHA;AAIA,YAAA,MAAA,EAAA;AAJA,WAAA;;AAMA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,cAAA,IAAA,EAAA;AAAA,aAAA;AACA,WAFA,EAEA,IAFA,CAAA;AAGA;;AAAA;AACA,OAfA;AAgBA,KAzBA;;AA2BA;AACA;AACA;AACA,IAAA,gBA9BA,4BA8BA,WA9BA,EA8BA,QA9BA,EA8BA;AACA;AACA,UAAA,WAAA,EAAA;AACA,YAAA,IAAA,GAAA,EAAA;AACA,QAAA,YAAA,CAAA;AACA,UAAA,OAAA,EAAA;AADA,SAAA,CAAA,CAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,KAAA,EAAA;AAAA;AAAA;AAAA;;AAAA;AACA,mCAAA,GAAA,CAAA,KAAA,8HAAA;AAAA,oBAAA,CAAA;AACA,gBAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAIA,YAAA,IAAA,GAAA,GAAA,CAAA,KAAA;AACA,WALA,MAKA;AACA,YAAA,IAAA,GAAA,EAAA;AACA;;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,UAAA,QAAA,CAAA,IAAA,CAAA;AACA,SAbA,EAaA,KAbA,CAaA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,KAAA,CAAA,GAAA;AACA,SAfA;AAgBA;AACA,KAnDA;;AAqDA;AACA,IAAA,YAtDA,wBAsDA,IAtDA,EAsDA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,UAAA,IAAA,CAAA,WAAA,IAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,YAAA,OAAA,GAAA,IAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,mBAAA;AAAA,UAAA,MAAA,EAAA;AAAA,uBAAA;AAAA;AAAA,SAAA;AACA;;AACA,UAAA,IAAA,CAAA,WAAA,IAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,YAAA,QAAA,GAAA,IAAA,CAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,aAAA;AAAA,UAAA,MAAA,EAAA;AAAA,uBAAA;AAAA;AAAA,SAAA;AACA;AACA,KAnEA;;AAqEA;AACA,IAAA,UAtEA,sBAsEA,cAtEA,EAsEA;AAAA;;AACA,WAAA,KAAA,CAAA,cAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,IAAA,cAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,cAAA;AACA,UAAA,cAAA,GAAA,KAAA;AACA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,cAAA,GAAA,IAAA;AACA,WAFA,EAEA,IAFA,CAAA;AAIA,cAAA,GAAA,GAAA,MAAA,CAAA,QAAA,CAAA,GAAA;AACA,cAAA,YAAA,GAAA,MAAA,CAAA,cAAA,CAAA,OAAA;AACA,cAAA,YAAA,GAAA,MAAA,CAAA,cAAA,CAAA,OAAA;AACA,UAAA,OAAA,CAAA,GAAA,WAAA,GAAA,cAAA,YAAA,cAAA,YAAA;AACA,UAAA,aAAA,CAAA;AACA,YAAA,GAAA,EAAA,GADA;AAEA,YAAA,YAAA,EAAA,YAFA;AAGA,YAAA,YAAA,EAAA;AAHA,WAAA,CAAA,CAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA;AACA,gBAAA,OAAA,EAAA,GAAA,CAAA,IADA;AAEA,gBAAA,IAAA,EAAA,SAFA;AAGA,gBAAA,QAAA,EAAA,IAHA;AAIA,gBAAA,MAAA,EAAA;AAJA,eAAA;;AAMA,cAAA,UAAA,CAAA,YAAA;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AACA;;AACA,gBAAA,MAAA,CAAA,MAAA;AACA,eAJA,EAIA,IAJA,CAAA;AAKA;;AACA,gBAAA,GAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA;AACA,gBAAA,OAAA,EAAA,GAAA,CAAA,IADA;AAEA,gBAAA,IAAA,EAAA,OAFA;AAGA,gBAAA,QAAA,EAAA,IAHA;AAIA,gBAAA,MAAA,EAAA;AAJA,eAAA;AAMA;AACA,WA1BA,EA0BA,KA1BA,CA0BA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WA5BA;AA6BA,SAxCA,MAwCA;AACA,UAAA,OAAA,CAAA,KAAA,CAAA,YAAA;AACA,iBAAA,KAAA;AACA;AACA,OA7CA;AA8CA,KArHA;;AAsHA;AACA,IAAA,SAvHA,qBAuHA,cAvHA,EAuHA;AACA,WAAA,KAAA,CAAA,cAAA,EAAA,WAAA;AACA,KAzHA;;AA2HA;AACA,IAAA,OA5HA,qBA4HA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA;AA9HA,IAnEA;AAoMA,EAAA,OApMA,qBAoMA;AACA,SAAA,WAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;AACA,GAvMA;AAyMA,EAAA,QAAA,oBACA,UAAA,CAAA;AACA,IAAA,QAAA,EAAA;AADA,GAAA,CADA;AAzMA,CAAA","sourcesContent":["<template>\r\n    <div class=\"headerTop-box\">\r\n        <div class=\"headerTop-user\">\r\n\r\n            <div class=\"header-cityName\" @click=\"goIndex()\">\r\n                <i class=\"el-icon-s-home\"></i>{{UserInfo.city_name || \"智迈科技\"}} - 后台首页\r\n            </div>\r\n\r\n            <!--头部搜索-->\r\n            <el-form class=\"search-form\">\r\n                <el-autocomplete\r\n                        placeholder=\"请输入姓名、卡号、会员卡号\"\r\n                        v-model=\"SearchVal\"\r\n                        :fetch-suggestions=\"querySearchAsync\"\r\n                        @select=\"handleSelect\"\r\n                        popper-class=\"search-content\"\r\n                        >\r\n                    <i slot=\"prefix\" class=\"el-input__icon el-icon-search\"></i>\r\n                    <template slot-scope=\"{ item }\">\r\n                         <div class=\"search-phone\">{{ item.phone }}</div>\r\n                         <span class=\"search-name\">{{ item.true_name }}</span>\r\n                     </template>\r\n                </el-autocomplete>\r\n\r\n            </el-form>\r\n\r\n            <!--头部用户信息+退出+更换密码 -->\r\n            <div class=\"headerTop-userInfo\">\r\n                <img class=\"userHeader\" :src='localUrl+\"/\"+UserInfo.logo || \"assets/images/logo-daka.png\"' alt=\"\">\r\n                <el-dropdown class=\"dropdown-header\">\r\n                    <div class=\"el-dropdown-link\">\r\n                        <div class=\"userName\">\r\n                            {{UserInfo.username}}\r\n                            <i class=\"el-icon-caret-bottom\"></i>\r\n                        </div>\r\n                    </div>\r\n                    <el-dropdown-menu slot=\"dropdown\"\r\n                                      hide-timeout=\"30000\"\r\n                                      class=\"dropdown-HeaderTop\">\r\n                        <el-dropdown-item command=\"a\">\r\n                            <el-button type=\"text\" @click=\"dialogFormVisible = true\">更换密码</el-button>\r\n                        </el-dropdown-item>\r\n                        <el-dropdown-item command=\"b\">\r\n                            <el-button type=\"text\" @click=\"loginOut()\">退出</el-button>\r\n                        </el-dropdown-item>\r\n                    </el-dropdown-menu>\r\n                </el-dropdown>\r\n            </div>\r\n        </div>\r\n\r\n        <!--更换密码弹出-->\r\n        <el-dialog title=\"更换密码\"\r\n                   :visible.sync=\"dialogFormVisible\"\r\n                   custom-class=\"passAlert\"\r\n                   width=\"600px\">\r\n            <el-form :model=\"changePassForm\" status-icon :rules=\"changeRules\" ref=\"changePassForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n                <el-form-item label=\"旧密码\" prop=\"oldPass\">\r\n                    <el-input type=\"password\" :show-password=\"true\" v-model=\"changePassForm.oldPass\" autocomplete=\"off\"  clearable></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"新密码\" prop=\"newPass\">\r\n                    <el-input type=\"password\" :show-password=\"true\" v-model=\"changePassForm.newPass\" autocomplete=\"off\"  clearable></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"确认密码\" prop=\"checkPass\">\r\n                    <el-input type=\"password\" :show-password=\"true\" v-model=\"changePassForm.checkPass\" autocomplete=\"off\"  clearable></el-input>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button type=\"primary\" size=\"small\" @click=\"submitForm('changePassForm')\">提交</el-button>\r\n                    <el-button size=\"small\" @click=\"resetForm('changePassForm')\">重置</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-dialog>\r\n\r\n        <!--<el-button size=\"small\" @click=\"reLoad()\">刷新</el-button>-->\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\n    import {ApiloginOut,ApiChangePass,ApiTopSearch} from '@/assets/js/api'\r\n\r\n    import {mapState,mapActions, mapGetters} from 'vuex'\r\n    let btnStateChange = true;  //修改密码按钮点击状态\r\n    export default {\r\n        /*在页面注入App.vue组件提供（provide）的 reload 依赖，在逻辑完成之后（删除或添加...）,\r\n        直接this.reload()调用，即可刷新当前页面*/\r\n        inject:['reLoad'], //注入依赖 App 中的reLoad方法\r\n        name: \"HeaderTop\",\r\n        data() {\r\n            /*旧密码验证规则*/\r\n            let validateOldPass= (rule, value, callback) => {\r\n                if (value === '') {\r\n                    callback(new Error('旧密码不能为空'));\r\n                } else if(value.length<=5){\r\n                    callback(new Error('密码不能少于6位数'));\r\n                }  else {\r\n                    callback();\r\n                }\r\n            };\r\n            /*新密码验证规则*/\r\n            let validateNewPass = (rule, value, callback) => {\r\n                if (value === '') {\r\n                    callback(new Error('新密码不能为空'));\r\n                } else if(value.length<=5){\r\n                    callback(new Error('密码不能少于6位数'));\r\n                }  else {\r\n                    callback();\r\n                }\r\n            };\r\n            /*确认密码验证规则*/\r\n            let validateCheckPass = (rule, value, callback) => {\r\n                if (value === '') {\r\n                    callback(new Error('请再次输入密码'));\r\n                } else if(value.length<=5){\r\n                    callback(new Error('密码不能少于6位数'));\r\n                } else if (value !== this.changePassForm.newPass) {\r\n                    callback(new Error('两次输入密码不一致!'));\r\n                } else {\r\n                    callback();\r\n                }\r\n            };\r\n\r\n            return {\r\n                localUrl:this.GLOBAL.localUrl,\r\n\r\n                SearchVal:\"\",  //搜索框值\r\n                dialogFormVisible: false,   //修改密码弹出框状态\r\n\r\n                formLabelWidth: '80px',\r\n\r\n                /*修改密码 form  changePassForm 对应v-model*/\r\n                changePassForm: {\r\n                    oldPass: '',\r\n                    newPass:'',\r\n                    checkPass: '',\r\n                },\r\n                /*修改密码 form  changeRules 对应 :rules*/\r\n                changeRules: {\r\n                    oldPass: [\r\n                        { validator: validateOldPass, trigger: 'blur' }\r\n                    ],\r\n                    newPass: [\r\n                        { validator: validateNewPass, trigger: 'blur' }\r\n                    ],\r\n                    checkPass: [  //确认密码\r\n                        { validator: validateCheckPass, trigger: 'blur' }\r\n                    ],\r\n                }\r\n            }\r\n        },\r\n        methods: {\r\n            ...mapActions({\r\n                ACTlogout:'StoreTagNav/ACTlogout',   //store里 loginOut 退出登录方法\r\n                metUserInfo:'StoreTagNav/actUserInfo',\r\n            }),\r\n\r\n            /*退出登录*/\r\n            loginOut(){\r\n                let that = this;\r\n                ApiloginOut().then(res=>{\r\n                    console.log(res);\r\n                    this.ACTlogout();\r\n                    if(res.status == 1){\r\n\r\n                        this.$message({\r\n                            type:'success',\r\n                            duration:1500,\r\n                            message:\"退出成功！\",\r\n                            offset:100,\r\n                        });\r\n                        setTimeout(()=>{\r\n                            that.$router.push({path:'/login'})\r\n                        },1500)\r\n                    };\r\n                })\r\n            },\r\n\r\n            /*搜索*/\r\n            //queryString 为在框中输入的值\r\n            //callback 回调函数,将处理好的数据推回\r\n            querySearchAsync(queryString, callback) {\r\n                // console.log(queryString);\r\n                if(queryString){\r\n                    let list =[];\r\n                    ApiTopSearch({\r\n                        keyword:queryString,\r\n                    }).then(res=>{\r\n                        if(res.value){\r\n                            for(let i of res.value){\r\n                                i.value = i.phone;\r\n                            }\r\n                            list = res.value;\r\n                        } else{\r\n                            list = [];\r\n                        }\r\n                        console.log(list);\r\n                        callback(list);\r\n                    }).catch(res=>{\r\n                        console.error(res);\r\n                    });\r\n                }\r\n            },\r\n\r\n            /*选中*/\r\n            handleSelect(item) {\r\n                console.log(item);\r\n                if(item.member_type == 1) {\r\n                    console.log('正式会员');\r\n                    let user_id = item.id;\r\n                    console.log(user_id);\r\n                    this.$router.push({name:'Memberreal_member',params:{'user_id':user_id,}});\r\n                }\r\n                if(item.member_type == 0){\r\n                    console.log('非正式会员');\r\n                    let user_id = item.id;\r\n                    this.$router.push({name:'Memberindex', params:{'user_id':user_id,}})\r\n                }\r\n            },\r\n\r\n            /*修改密码提交*/\r\n            submitForm(changePassForm) {\r\n                this.$refs[changePassForm].validate((valid) => {\r\n                    if (valid && btnStateChange) {\r\n                        console.log(btnStateChange);\r\n                        btnStateChange = false;\r\n                        setTimeout(()=>{\r\n                            btnStateChange = true;\r\n                        },1000);\r\n\r\n                        let uid = this.UserInfo.uid;\r\n                        let old_password = this.changePassForm.oldPass;\r\n                        let new_password = this.changePassForm.newPass;\r\n                        console.log(`${uid},${old_password},${new_password}`);\r\n                        ApiChangePass({\r\n                            uid:uid,\r\n                            old_password:old_password,\r\n                            new_password:new_password,\r\n                        }).then(res =>{\r\n                            if(res.status == 1){\r\n                                this.$message({\r\n                                    message: res.info,\r\n                                    type: 'success',\r\n                                    duration:1500,\r\n                                    offset:100,\r\n                                });\r\n                                setTimeout(()=>{\r\n                                    this.dialogFormVisible = false;\r\n                                    /*刷新页面*/\r\n                                    this.reLoad();\r\n                                },1500)\r\n                            }\r\n                            if(res.status == 0){\r\n                                this.$message({\r\n                                    message: res.info,\r\n                                    type: 'error',\r\n                                    duration:3000,\r\n                                    offset:40,\r\n                                });\r\n                            }\r\n                        }).catch(res=>{\r\n                            console.log(res);\r\n                        })\r\n                    } else {\r\n                        console.error('确认密码提交失败!!');\r\n                        return false;\r\n                    }\r\n                });\r\n            },\r\n            /*修改密码 重置*/\r\n            resetForm(changePassForm) {\r\n                this.$refs[changePassForm].resetFields();\r\n            },\r\n\r\n            /*回到首页*/\r\n            goIndex(){\r\n                this.$router.push({path:'/index'});\r\n            },\r\n\r\n        },\r\n        created() {\r\n            this.metUserInfo();\r\n            console.log(this.UserInfo);\r\n        },\r\n\r\n        computed: {\r\n            ...mapGetters({\r\n                UserInfo:'StoreTagNav/getsUserInfo'\r\n            })\r\n        },\r\n    }\r\n</script>\r\n"],"sourceRoot":"src/components/layout"}]}